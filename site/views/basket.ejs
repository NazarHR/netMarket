   <script>
    $(document).ready(content());
    function content () { 
        var values = document.cookie.replace(/(?:(?:^|.*;\s*)basket-data\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        $.ajax({
            url: "/basket",
            type: 'put',
            data: {data:values},
            success:function loader(data){$("#basket").html(data);console.log("works");}
        })
        setTimeout(content, 30000);
    }
    function del(name)
    {
        $.ajax({
            url:'/buskdel',
            type: 'post',
            data: {toDel:name}
        }).done(content());
    }
    function add_all()
