<ul>
    <% for(let i=0; i<products.length; ++i) { %>
        <li>
            <img src= "../image/1.jpg" > <%=products[i].name %>
            <%if(products[i].discount==0){%> <%=products[i].price%>
                <%}else {%> <%=Math.ceil(((products[i].price*(1-products[i].discount/100)*100)/100))%><%}%>
            <a class="addbtn" type="submit", onmousedown="del('<%=products[i].name%>')">прибрати з корзини</a>
        </li>
    <%} %>
</ul>
<a class="addbtn" onmousedown="add_all()">купити все</a>